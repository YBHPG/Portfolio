    <title>
        <%= __('work-page-title') %>
    </title>
    <link rel="icon" type="image/x-icon" href="/static/images/other/favicon.ico" />

    <script src="/static/javascript/lazyVideo.js"></script>
</head>

<body class="body_white">
    <div class="cursor"></div>
    <div class="trailing-cursor" id="trailing-cursor"></div>
    <script src="/static/javascript/TransitionAnimations.js"></script>
    <main>
        <header class="header">
            <div class="header__container">
                <div class="header__logo">
                </div>
                <div class="header__menu">
                    <div class="menu__body">
                        <ul class="menu__list">
                            <li class="menu__item">
                                <a href="#" class="menu__link text-black" onclick="fadeInBurger()">
                                    <%= __('header-work') %>
                                </a>
                            </li>
                            <li class="menu__item">
                                <a href="#" class="menu__link text-black" id="fade-out_work-button"
                                    onclick="fadeOutWork()">
                                    <%= __('header-about') %>
                                </a>
                            </li>
                            <li class="menu__item">
                                <a onclick="resumeLink()" target="_blank" class="menu__link text-black">
                                    <%= __('header-resume') %>
                                </a>
                            </li>
                            <li class="menu__item">
                                <a target="" class="menu__link text-black" onclick="changeLanguage('userLang')">
                                    <%= __('header-switch-language') %>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="burger-button">
                        <a href="#" class="burger-button_link" onclick="fadeInBurger()">
                            <span class="burger-button_line"></span>
                        </a>
                    </div>
                </div>
            </div>
        </header>
        <section class="block-1">
            <div class="block-1__container">
                <div class="block-1__content">
                    <h1 class="block-1__title text__main-title text__italic">
                        <%= __('block-1-title') %>
                    </h1>
                    <div class="block-1__text">
                        <p class="text__text text-center">
                            <%= __('block-1-subtitle-1') %>
                        </p>
                        <p class="text__text text-center">
                            <%= __('block-1-subtitle-2') %>
                        </p>
                        <p class="text__text text-center">
                            <%= __('block-1-subtitle-3') %> <a class="text__link"
                                    href="https://www.behance.net/gallery/143770851/Dicty-App-translator-and-dictionary"
                                    target="_blank">
                                    Dicty</a>
                                <%= __('block-1-subtitle-4') %>
                        </p>
                    </div>
                </div>
            </div>
        </section>
        <section class="block-2">
            <div class="block-2__container">
                <div class="block-2__content">
                    <div class="block-2__separator"></div>
                    <p class="block-2__text text__header">
                        <%= __('block-2-title') %>
                    </p>
                    <div class="block-2__filter-buttons__overflow">
                        <div class="block-2__filter-buttons__container">
                            <a href="#!" class="block-2__filter-buttons" onclick="dataSort('all')">
                                <%= __('block-2-filter-all') %>
                            </a>
                            <a href="#!" class="block-2__filter-buttons" onclick="dataSort('design')">
                                <%= __('block-2-filter-design') %>
                            </a>
                            <a href="#!" class="block-2__filter-buttons" onclick="dataSort('programming')">
                                <%= __('block-2-filter-programming') %>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <div class="work-blocks__container" id="work-blocks">
            <section class="work-block-test is_visible" data-sort="programming">
                <div class="work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title"><a class="text__link" href="/tiles-demo" target="_blank">Tiles</a>
                                <%= __('work-block-tiles-title') %>
                            </h1>
                            <span>
                                <p class="text__text">
                                    <%= __('work-block-tiles-text') %>
                                </p>
                            </span>
                        </div>
                        <div class="work-block-test__images">
                            <video id="tiles-video" playsinline autoplay muted loop
                                src="/static/images/index/tiles-video_1.mp4"
                                poster="/static/images/index/video-preview-poster.webp"></video>
                        </div>
                    </div>
                </div>
            </section>
            <section class="work-block-test is_visible" data-sort="design">
                <div class="work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title"><a class="text__link"
                                    href="https://www.behance.net/gallery/143770851/Dicty-App-translator-and-dictionary"
                                    target="_blank">Dicty</a>
                                <%= __('work-block-dicty-title') %>
                            </h1>
                            <div>
                                <p class="text__text">
                                    <%= __('work-block-dicty-text-1') %>
                                </p><br>
                                <p class="text__text">
                                    <%= __('work-block-dicty-text-2') %>
                                </p><br>
                                <p class="text__text">
                                    <%= __('work-block-dicty-text-3') %>
                                </p><br>
                                <p class="text__text">
                                    <%= __('work-block-dicty-text-4') %>
                                </p><br>
                            </div>
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/dicty-image_1.webp"
                                alt="Floating phone in air">
                            <img loading="lazy" src="/static/images/index/dicty-image_2.webp"
                                alt="Three different app screens levitating in air">
                        </div>
                    </div>
                </div>
            </section>
            <section class="work-block-test is_visible" data-sort="design">
                <div class="work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title"><a class="text__link"
                                    href="https://www.behance.net/gallery/125033977/Rezolut-internet-company-landing-page"
                                    target="_blank">Rezolut</a>
                                <%= __('work-block-rezolut-title') %>
                            </h1>
                            <span>
                                <p class="text__text">
                                    <%= __('work-block-rezolut-text') %>
                                </p><br>
                            </span>
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/rezolut-image_1.webp"
                                alt="Rezolut landing blocks presentation">
                            <img loading="lazy" src="/static/images/index/rezolut-image_2.webp"
                                alt="Front page of Rezolut landing">
                        </div>
                    </div>
                </div>
            </section>
            <section class="work-block-test is_visible" data-sort="design">
                <div class="work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title"><a class="text__link"
                                    href="https://www.behance.net/gallery/125037427/CarrotIT-landing-page-concept"
                                    target="_blank">CarrotIT</a>
                                <%= __('work-block-carrotit-title') %>
                            </h1>
                            <span>
                                <p class="text__text">
                                    <%= __('work-block-carrotit-text') %>
                                </p>
                                <br>
                            </span>
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/carrot_it-image_1.webp"
                                alt="Floating phone in air">
                        </div>
                    </div>
                </div>
            </section>
            <section class="work-block-test is_visible" data-sort="design">
                <div class=" work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title">
                                <a class="text__link"
                                    href="https://www.behance.net/gallery/125101961/RentiOne-company-landing-page"
                                    target="_blank">RentiOne</a>
                                <%= __('work-block-rentione-title') %>
                            </h1>
                            <span>
                                <p class="text__text">
                                    <%= __('work-block-rentione-text') %>
                                </p>
                                <br>
                            </span>
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/rentione-image-1.webp" alt="">
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/rentione-image-2.webp" alt="">
                        </div>
                    </div>
                </div>
            </section>
            <section class="work-block-test is_visible" data-sort="design">
                <div class="work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title">
                                <a class="text__link"
                                    href="https://www.behance.net/gallery/159726407/Tyshop-typography-shop-landing-page"
                                    target="_blank">Tyshop</a>
                                <%= __('work-block-tyshop-title') %>
                            </h1>
                            <div>
                                <p class="text__text">
                                    <%- __('work-block-tyshop-text') %>
                                </p>
                                <br>
                            </div>
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/typography-image_1.webp" alt="">
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/typography-image_2.webp" alt="">
                        </div>
                    </div>
                </div>
            </section>
            <section class="work-block-test is_visible" data-sort="design">
                <div class="work-block-test__container">
                    <div class="work-block-test__content">
                        <div class="work-block-test__text">
                            <h1 class="text__title">
                                <a class="text__link" href="https://www.behance.net/ybhpg/projects" target="_blank">
                                    <%= __('work-block-graphics-design-title') %>
                                </a>
                            </h1>
                            <div>
                                <p class="text__text">
                                    <%- __('work-block-graphics-design-text') %>
                                </p>
                                <br>
                            </div>
                        </div>
                        <div class="work-block-test__images">
                            <img loading="lazy" src="/static/images/index/graphics-design-image_1.webp" alt="">
                        </div>
                    </div>
                </div>
            </section>
        </div>

        <a href="#" class="top_button" onclick="topFunction()">
            <div class="top_button__container">
                <img loading="lazy" src="/static/images/other/icons8-up-2-48.webp" alt="arrow up"
                    class="top-button__image" />
            </div>
        </a>
    </main>
    <footer>
        <div class="footer__container">
            <div class="footer__content">
                <div class="footer__main-block">
                    <div class="footer__greetings">
                        <p class="text__text">
                            <%= __('footer-greeting') %>
                        </p>
                        <p class="text__text">
                            <%= __('footer-email-1') %>
                                <a class="text__link" href="mailto:hello@vladl.space">
                                    <%= __('footer-email-2') %>
                                </a>
                                <%= __('footer-email-3') %>
                        </p>
                    </div>
                    <div class="footer__copyrights text__text">
                        <%= __('footer-copyright') %>
                    </div>
                </div>
                <div class="footer__socials">
                    <ul class="footer__socials-list">
                        <li class="footer__socials socials-variant">
                            <a href="https://t.me/ybhpg" class="social_link" target="_blank">Telegram</a>
                        </li>
                        <li class="socials-variant">
                            <p class="text__text">|</p>
                        </li>
                        <li class="socials-variant">
                            <a href="https://www.instagram.com/bulbadyshka/" class="social_link"
                                target="_blank">Instagram</a>
                        </li>
                        <li class="socials-variant">
                            <p class="text__text">|</p>
                        </li>
                        <li class="socials-variant">
                            <a href="https://twitter.com/ybhpg57/" class="social_link" target="_blank">Twitter</a>
                        </li>
                        <li class="socials-variant">
                            <p class="text__text">|</p>
                        </li>
                        <li class="socials-variant">
                            <a href="https://www.linkedin.com/mwlite/in/ybhpg" class="social_link"
                                target="_blank">LinkedIn</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <script src="/static/javascript/backToTopButton.js"></script>
    <script src="/static/javascript/cursorAnimation.js"></script>
    <script src="/static/javascript/filterButtons.js"></script>
    <script src="/static/javascript/themeChange.js"></script>
    <script src="/static/javascript/changeLanguage.js"></script>
    </div>
</body>

</html>